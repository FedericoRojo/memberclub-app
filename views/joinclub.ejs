<html>
  <head>
    <title>Join club</title>
    <link rel="stylesheet" type="text/css" href="/styles/styleFormPage.css"> 
  </head>
  <body>
    
    <h1>Join club</h1>

    <% if(error) { %>
      <p> <%= error %> </p>
    <% } %>

    <form method="POST" action="/joinclub">
        <label for="name">Enter pass key:</label>
        <input type="text" id="passkey" name="passkey">
        <button type="submit">Submit</button>
    </form>
    
    <a href="/">
      <span>Home</span>
    </a>
  </body>
</html>



